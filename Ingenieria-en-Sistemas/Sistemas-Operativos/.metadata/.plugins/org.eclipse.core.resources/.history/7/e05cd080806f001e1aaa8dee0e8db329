package LaPiedraOscilante;

import java.util.LinkedList;
import java.util.Queue;

public class Parque {
	int visitantes;
	int maxCoches;
	Queue<Coche> cochesDisponibles;
	Queue<Visitante> visitantesEsperando;
	
	public Parque(int visitantes,int maxCoches){
		this.visitantes = visitantes;
		this.maxCoches = maxCoches;
		cochesDisponibles = new LinkedList<Coche>();
		visitantesEsperando = new LinkedList<Visitante>();
	}
	
	public synchronized Coche hayCochesLibres() {
		while (!cochesDisponibles.isEmpty()) {
            Coche elemento = cochesDisponibles.peek();
            if(elemento.esCocheLibre() != null) {
            	elemento.ocuparCoche();
            	System.out.println();
            }
        }
		return null;
	}
}
