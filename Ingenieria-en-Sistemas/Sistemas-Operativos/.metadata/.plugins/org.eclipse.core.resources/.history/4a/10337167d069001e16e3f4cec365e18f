package ArrayMedia;

public class ThreadMedia implements Runnable{
	
	private int[] array = new int[10000];
	private int inicio;
	private int fin;
	private int resultado;
	
	@Override
	public void run() {
		int suma = 0;
		for(int i = inicio; i < fin;i++ ) {
			suma += array[i];
		}
		resultado = suma/(fin - inicio);
	}

}
