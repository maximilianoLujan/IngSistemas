package ProductorConsumidor;

public class main {

	public static void main(String[] args) {
		
		IBuffer<Integer> i= new SynchronizedBuffer<>(10);
		
		
		Productor p = new Productor(100,i,1000);
		Consumidor c = new Consumidor(i,1000);
		
		p.setName("Productor");
		c.setName("Consumidor");
		
		
		p.start();
		c.start();
		

	}

}
