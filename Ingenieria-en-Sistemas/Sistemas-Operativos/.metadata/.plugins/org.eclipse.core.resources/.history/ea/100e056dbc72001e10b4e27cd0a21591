package EjercicioBlancaNieves;

public class Casa {
	int maximoSillas;
	int sillasOcupadas;
	
	
	public Casa(int maximoSillas) {
		this.maximoSillas = maximoSillas;
		this.sillasOcupadas = 0;
	}
	
	public synchronized boolean ocuparSilla() {
		if (sillasOcupadas == maximoSillas) {
			return false;
		}
		
	}
	
}
