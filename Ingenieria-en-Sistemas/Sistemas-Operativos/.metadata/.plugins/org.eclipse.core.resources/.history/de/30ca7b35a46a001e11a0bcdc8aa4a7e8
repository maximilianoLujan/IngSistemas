package SumTwoArraysWithThreads;

public class ThreadSum extends Thread{
	
	private int[] array;
	private int from;
	private int to;
	private int result;
	
	
	public ThreadSum(int[] arr,int inic,int fin) {
		super();
		this.array = arr;
		this.from = inic;
		this.to = fin;
		this.result = 0;
	}
	
}
