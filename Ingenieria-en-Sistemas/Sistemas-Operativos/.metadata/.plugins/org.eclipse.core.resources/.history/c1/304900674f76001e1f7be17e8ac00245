package BlancanievesSieteEnanitos;


public class Main {

	public static void main(String[] args) throws InterruptedException {
		Blancanieves b1 = new Blancanieves();
		
		Casa casa = new Casa(4,0);
		
		for(int i = 0; i < 7;i++) {
			Enanito en = new Enanito(casa);
			Thread.sleep(1000);
			en.start();
		}

	}

}
